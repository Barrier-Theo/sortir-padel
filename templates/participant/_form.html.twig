{{ form_start(form) }} 
<div class="row">
    <div class="col-12">{{ form_row(form.pseudo) }}</div>
    <div class="col-6">{{ form_row(form.nom) }}</div>
    <div class="col-6">{{ form_row(form.prenom) }}</div>
    <div class="col-6">{{ form_row(form.telephone) }}</div>
    <div class="col-6">{{ form_row(form.mail) }}</div>

</div>

    {{ form_widget(form) }}
    <button class="btn btn-primary mb-2">{{ button_label|default('Enregistrer') }}</button>
{{ form_end(form) }}
